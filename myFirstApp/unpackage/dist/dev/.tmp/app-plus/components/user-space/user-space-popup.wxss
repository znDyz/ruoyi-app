
.paper-left-popup-mask{
	position: fixed;
	right: 0;
	left: 0;
	top: 0;
	bottom: 0;
	z-index: 1999;
}
.paper-left-popup{
	position: fixed;
	right: 0;
	top: 120rpx;
	background: #FFFFFF;
	width: 55%;
	z-index: 2000;
	box-shadow: 1rpx 1rpx 15rpx 2rpx #CCCCCC;
}
.paper-left-popup>view{
	padding: 20rpx;
	font-size: 35rpx;
}
.paper-left-popup>view>view{
	margin-right: 10rpx;
	font-weight: bold;
}
.paper-left-popup-hover{
	background: #EEEEEE;
}

